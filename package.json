{
  "name": "vscode-vboy",
  "displayName": "Vitest Runner",
  "version": "0.0.4",
  "main": "dist/index.js",
  "icon": "logo.png",
  "license": "MIT",
  "description": "Vitest case runner.",
  "publisher": "kingwl",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Testing",
    "Other"
  ],
  "keywords": [
    "vitest",
    "runner",
    "codelens"
  ],
  "repository": "git@github.com:small-cosmos/vscode-vboy.git",
