    "onLanguage:javascript",
    "onLanguage:typescriptreact",
    "onLanguage:javascriptreact"
  ],
  "devDependencies": {
    "@types/node": "^18.8.5",
    "@types/vscode": "^1.72.0",
    "esbuild": "^0.15.10",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "vitest": "^0.24.1",
    "vsce": "^2.11.0"
  },
  "scripts": {
    "build": "node ./scripts/build.js",
    "package": "pnpm build && pnpm vsce package",
